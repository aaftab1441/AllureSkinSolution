    <script>
        $(document).ready(function () {
            $(".owl-carousel").owlCarousel({
                items: 1,
                margin: 60,
                loop: true,
                dotClass: 'owl-dot',
                dotsClass: 'owl-dots',
                dots: true,
                autoplay: true,
                responsiveClass: true,
                navText: ['<i class="fa fa-chevron-left"></i>', '<i class="fa fa-chevron-right"></i>'],
                responsive: {
                    // breakpoint from 0 up
                    0: {
                        items: 1,
                        margin: 0
                    },
                    // breakpoint from 480 up
                    480: {
                        items: 1,
                        margin: 0
                    },
                    // breakpoint from 768 up
                    768: {
                        items: 1,
                        margin: 0
                    },
                    // breakpoint from 768 up
                    1200: {
                        items: 2,
                        margin: 60
                    }
                }

            });
        });
